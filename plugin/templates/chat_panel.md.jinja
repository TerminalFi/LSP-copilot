<style>
  html {
    --copilot-delete-foreground: var(--redish);
    --copilot-delete-background: var(--background);
    --copilot-delete-border: var(--redish);

    --copilot-delete-conversation-foreground: color(var(--redish) a(0.75));
    --copilot-delete-conversation-background: var(--background);
    --copilot-delete-conversation-border: color(var(--redish) a(0.75));

    --copilot-close-foreground: var(--foreground);
    --copilot-close-background: var(--background);
    --copilot-close-border: var(--foreground);

    --copilot-accept-foreground: var(--foreground);
    --copilot-accept-background: var(--background);
    --copilot-accept-border: var(--greenish);
  }

  .wrapper {
    margin: 1rem 0.5rem 0 0.5rem;
  }

  .wrapper .suggested-title {
    display: inline-block;
    text-size: 1.2em;
    margin-bottom: 1rem;
    padding-bottom: 1rem;
  }

  .wrapper .navbar {
    text-align: left;
    margin-top: 1rem;
    margin-bottom: 1rem;
  }


  .wrapper .header {
    display: block;
    margin-bottom: 1rem;
  }

  .wrapper a {
    border-radius: 3px;
    border-style: solid;
    border-width: 1px;
    display: inline;
    padding: 2px;
    text-decoration: none;
  }

  .wrapper a.follow-up {
    border-radius: 0px;
    border-style: none;
    border-width: 0px;
    display: inline;
    text-decoration: underline;
  }

  .wrapper a.rating {
    border-radius: 0px;
    border-style: none;
    border-width: 0px;
    display: inline;
    padding: 2px;
    text-decoration: none;
  }

  .wrapper a.delete {
    border-radius: 0px;
    border-style: none;
    border-width: 0px;
  }

  .wrapper a.delete i {
    color: var(--copilot-delete-border);
  }

  .wrapper a.delete-conversation {
    background: var(--copilot-delete-conversation-background);
    border-color: var(--copilot-delete-conversation-border);
    color: var(--copilot-delete-conversation-foreground);
  }

  .wrapper a.delete-conversation i {
    color: var(--copilot-delete-conversation-border);
  }

  .wrapper a.close {
    background: var(--copilot-close-background);
    border-color: var(--copilot-close-border);
    color: var(--copilot-close-foreground);
  }

  .wrapper a.close i {
    color: var(--copilot-close-border);
  }

  .wrapper a.accept {
    background: var(--copilot-accept-background);
    border-color: var(--copilot-accept-border);
    color: var(--copilot-accept-foreground);
  }

  .wrapper a.accept {
    background: var(--copilot-accept-background);
    border-color: var(--copilot-accept-border);
    color: var(--copilot-accept-foreground);
  }

  .wrapper a.accept i {
    color: var(--copilot-accept-border);
  }

  .wrapper .kind {
    padding: 1px;
    color: color(var(--greenish) alpha(1));
  }

  .wrapper .kind.report {
    color: color(var(--yellowish) alpha(1));
  }

  .wrapper .icon {
    width: 1.25em;
    height: 1.25em;
    color: var(--foreground)
  }
</style>


<h3 class="suggested-title">
Copilot Chat {% if suggested_title %}| {{ suggested_title }}{% endif %}
</h3>


<div class="navbar">
  <a class="delete-conversation" title="Delete Conversation" href='{{ delete_url }}'><i>√ó</i> Delete Conversation</a>
  <a class="close" title="Close Conversation" href='{{ close_url }}'><i>√ó</i> Close Conversation</a>
</div>

{% for section in sections %}

---

<div class="header">
{% if section.kind == "report" %}
  <a class="rating" title="üëç" href='{{ section.thumbs_up_url }}'>üëç</a>
  <a class="rating" title="üëé" href='{{ section.thumbs_down_url }}'>üëé</a>
{% else %}
  <a class="delete" title="Delete Turn" href='{{ section.turn_delete_url }}'><img class="icon" src='data:image/png;base64,'/></a>
{% endif %}
</div>

<span class="kind {{ section.kind }}">{% if section.kind == "report" %}<img class="icon" src='data:image/png;base64,'/> Github Copilot{% else %}{{ section.kind }}{% endif %}:</span>

{{ section.message }}

{% endfor %}

{% if follow_up != "" %}
Follow up: <a class="follow-up" href="#">{{ follow_up }}</a>
{% endif %}
